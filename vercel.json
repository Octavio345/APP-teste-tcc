{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/vite-pwa/(.*)",
      "dest": "/vite-pwa/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}